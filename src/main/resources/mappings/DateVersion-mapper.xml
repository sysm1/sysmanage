      )
  </insert>
  
  <insert id="add" parameterType="DateVersion" useGeneratedKeys="true" keyProperty="id" >
    insert into date_version
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="dateK != null" >
        date_k,
      </if>
      <if test="valueV != null" >
        value_v,
