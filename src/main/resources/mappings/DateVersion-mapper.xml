		<include refid="selectId" />
		from date_version
		order by id desc
	</select>
  
  <select id="getById" parameterType="String" resultType="ClothAllowance" >
   
    select 
    <include refid="selectId" />
    from date_version
    where id = #{id}
  </select>
  
  <delete id="delete" parameterType="String" >
