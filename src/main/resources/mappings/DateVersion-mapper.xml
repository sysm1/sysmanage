      </if>
      <if test="valueV != null" >
        #{valueV,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
  
  <update id="update" parameterType="DateVersion" >
    update date_version
    <set >
      <if test="dateK != null" >
        date_k = #{dateK,jdbcType=VARCHAR},
      </if>
      <if test="valueV != null" >
        value_v = #{valueV,jdbcType=INTEGER},
      </if>
    </set>
    where id = #{id}
